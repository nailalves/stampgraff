<%= simple_form_for(@o) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :cliente_id %>
    <%= f.input :user_id %>
    <%= f.input :produto_id %>
    <%= f.input :descricao %>
    <%= f.input :valor %>
    <%= f.input :criacao %>
    <%= f.input :observacao %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
