<%= simple_form_for(@cliente) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :rg %>
    <%= f.input :nascimento %>
    <%= f.input :telefone1 %>
    <%= f.input :telefone2 %>
    <%= f.input :celular %>
    <%= f.input :endereco %>
    <%= f.input :email %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
